<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<Authors>YiJingFramework</Authors>
    <Company>YiJingFramework</Company>
    <Product>SixRens.DefaultPlugins</Product>
    <PackageId>SixRens.DefaultPlugins</PackageId>
    <Version>1.13.1</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/YiJingFramework/sixrens-defaultplugins.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>zh-Hans</NeutralLanguage>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SixRens.Api" Version="1.17.1" />
    <PackageReference Include="SixRens.Tools" Version="1.1.0" />
  </ItemGroup>

</Project>
